    <script type="text/javascript" src="${request.static_url('micropsi_server:static/js/paper.js')}"></script>
    <script type="text/paperscript" src="${request.static_url('micropsi_server:static/js/nodenet.js')}" canvas="nodenet"></script>
    </metal:script>
